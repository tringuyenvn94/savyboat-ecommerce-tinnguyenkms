<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="Library.DTO" namespace="Library.DTO" xmlns="urn:nhibernate-mapping-2.2">
  <class name="ContactUs" table="ContactUs" lazy="true" >
    <id name="Id" column="Id" type="Guid" >
      <generator class="guid" />      
    </id>
    <property name="Name">
      <column name="Name" sql-type="nvarchar" not-null="false" />
    </property>
    <property name="Email">
      <column name="Email" sql-type="nvarchar" not-null="false" />
    </property>
    <property name="Telephone">
      <column name="Telephone" sql-type="nvarchar" not-null="false" />
    </property>
    <property name="Subject">
      <column name="Subject" sql-type="nvarchar" not-null="false" />
    </property>
    <property name="Comments">
      <column name="Comments" sql-type="nvarchar" not-null="false" />
    </property>
    <property name="CreatedDate">
      <column name="CreatedDate" sql-type="datetime" not-null="false" />
    </property>
    <property name="Status">
      <column name="Status" sql-type="int" not-null="false" />
    </property>
  </class>
</hibernate-mapping>